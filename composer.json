{
    "name": "lautarolubatti/seoscraper",
    "description": "A SEO Web Scrap tool",
    "type": "library",
    "keywords": [
        "seo",
	"scrap"
    ],
    "homepage": "https://github.com/lautarolubatti/seoscraper/",
    "license": "MIT",
    "authors": [
        {
            "name": "Lautaro Lubatti",
            "email": "lr.lubatti@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/lautarolubatti/seoscraper/issues"
    },
    "require": {
        "php": ">=7.1",
        "symfony/dom-crawler": "^4.2",
        "guzzlehttp/guzzle": "^6.3",
        "league/container": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "autoload": {
        "psr-4": {
            "SeoScraper\\": "src/"
        },
        "exclude-from-classmap": ["tests/"]
    }
}
